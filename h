/* =========================
   GLOBAL RESET & THEME
========================= */
* {
  box-sizing: border-box;
  margin: 0;
  padding: 0;
}

body {
  font-family: "Segoe UI", system-ui, -apple-system, BlinkMacSystemFont, sans-serif;
  background: var(--bjp-gradient-soft);
  color: #222;
  scroll-behavior: smooth;
}

/* =========================
   BJP THEME VARIABLES
========================= */
:root {
  --bjp-saffron: #ff6600;
  --bjp-saffron-light: #ffb347;
  --bjp-white: #ffffff;
  --bjp-green: #1a7f37;
  --dark: #111;

  --bjp-gradient-main: linear-gradient(
    135deg,
    #ff6600 0%,
    #ffb347 45%,
    #ffffff 75%,
    #e8f5e9 100%
  );

  --bjp-gradient-soft: linear-gradient(
    180deg,
    #fff4e6,
    #ffffff
  );

  --bjp-gradient-dark: linear-gradient(
    135deg,
    #ff6600,
    #ff8c00,
    #1a7f37
  );
}

/* =========================
   LOTUS STRIP
========================= */
.lotus-strip {
  position: absolute;
  top: 20px;
  left: 50%;
  transform: translateX(-50%);
  z-index: 1000;
